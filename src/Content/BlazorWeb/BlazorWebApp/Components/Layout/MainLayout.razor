@inherits LayoutComponentBase

<MApp>
@*#if (InteractiveAtRoot)
    <NavMenu />
##elseif (UseServer && UseWebAssembly)
    <NavMenu @rendermode="@InteractiveAuto" />
##elseif (UseServer)
    <NavMenu @rendermode="@InteractiveServer" />
##elseif (UseWebAssembly)
    <NavMenu @rendermode="@InteractiveWebAssembly" />
##endif*@
    <MMain>
        <MContainer Fluid>
            @Body
        </MContainer>
    </MMain>
</MApp>

@*#if (SampleContent && !InteractiveAtRoot)
<MSsrPageScript />

##endif*@
@*#if (UseServer || UseWebAssembly) -->
<div id="blazor-error-ui" data-nosnippet>
    An unhandled error has occurred.
    <a href="." class="reload">Reload</a>
    <span class="dismiss">🗙</span>
</div>
##endif*@
